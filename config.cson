"*":
  core:
    disabledPackages: [
      "linter-clang"
    ]
    telemetryConsent: "limited"
    themes: [
      "seti-ui"
      "seti-syntax"
    ]
    useTreeSitterParsers: false
  editor:
    fontFamily: "monospace"
    fontSize: 17
    scrollPastEnd: true
    showInvisibles: true
    tabLength: 4
  "exception-reporting":
    userId: "66f0e2b4-e130-79fd-dae4-80dc0d9e6cea"
  kite:
    showWelcomeNotificationOnStartup: false
  "linter-clang":
    clangDefaultCppFlags: "-Wall -Wextra -std=c++11"
    executablePath: "clang-5.0"
  "linter-flake8":
    ignoreErrorCodes: [
      "E302"
      "E128"
    ]
    projectConfigFile: [
      ".flake8rc"
    ]
  "linter-gcc":
    gccDefaultCFlags: "-c -Wall -fsyntax-only"
    gccDefaultCppFlags: "-c -Wall -std=c++11 -fsyntax-only"
  "linter-rust":
    cargoCommand: "rustc"
    disableExecTimeout: true
  "linter-ui-default":
    showPanel: true
  "markdown-themeable-pdf":
    customFooterPath: "markdown-themeable-pdf/uni-footer.js"
    customHeaderPath: "markdown-themeable-pdf/uni-header.js"
  "package-generator":
    packageSyntax: "coffeescript"
  "seti-ui":
    compactView: true
    displayIgnored: false
    fileIcons: false
    font: "Default Atom Font"
    themeColor: "Steel"
  "terminal-plus":
    core: {}
    style:
      defaultPanelHeight: "200px"
  welcome:
    showOnStartup: false
".lisp.source":
  editor:
    softWrap: false
